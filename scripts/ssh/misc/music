#!/usr/bin/env bash
#===================================================================================
#     FILE: misc/music
#   AUTHOR: Leonardo Marco (labadmin@leonardomarco.com)
#  LICENSE: GNU General Public License v3.0
#===================================================================================
script_id="$(echo ${BASH_SOURCE[0]} | sed 's\^'"$scripts_path"'/*\\g')"		# Script filename with relative script path

#=== LOAD INFO =====================================================================
if [ "$1" = "--load-scriptinfo" ]; then
	script_desc="The best moeldies in your pcspeaker! :D"
	script_type="ssh"
	script_bg="sub"
	script_admindeps=""
	script_hostdeps="beep"
	load_scriptinfo "$script_id" "$script_desc" "$script_type" "$script_bg" "$script_admindeps" "$script_hostdeps"
	return

#=== READ PARAMS ===================================================================
elif [ "$1" = "--readparams" ]; then
	# PARAM1: melody to play
	read_param "$script_id" 1 '1 Mario Bros Intro
2 Mario Victory
3 Monkey Island
4 Superdetective en Hollywood
5 Ring' "" "^(1|2|3|4|5)$"
	return
fi


#=== EXEC SCRIPT ===================================================================
which beep &> /dev/null || apt-get install beep
modprobe pcspkr

o="$1"

case "$o" in
	# MARIO-INTRO
	1) 	beep -n -f 146 -l 20 -D 0 -n -f 369 -l 20 -D 0 -n -f 659 -l 20 -D 47 -n -f 146 -l 20 -D 0 -n -f 369 -l 20 -D 0 -n -f 659 -l 20 -D 189 -n -f 146 -l 20 -D 0 -n -f 369 -l 20 -D 0 -n -f 659 -l 20 -D 190 -n -f 146 -l 20 -D 0 -n -f 369 -l 20 -D 0 -n -f 523 -l 20 -D 47 -n -f 146 -l 20 -D 0 -n -f 369 -l 20 -D 0 -n -f 659 -l 20 -D 190 -n -f 391 -l 20 -D 0 -n -f 493 -l 20 -D 0 -n -f 783 -l 20 -D 475 -n -f 195 -l 20 -D 0 -n -f 391 -l 20 -n -f 195 -l 20 -D 0 -n -f 391 -l 20 -D 473 -n -f 195 -l 20 -D 0 -n -f 329 -l 20 -D 0 -n -f 523 -l 20 -D 332 -n -f 164 -l 20 -D 0 -n -f 261 -l 20 -D 0 -n -f 391 -l 20 -D 332 -n -f 130 -l 20 -D 0 -n -f 195 -l 20 -D 0 -n -f 329 -l 20 -D 332 -n -f 174 -l 20 -D 0 -n -f 261 -l 20 -D 0 -n -f 440 -l 20 -D 189 -n -f 195 -l 20 -D 0 -n -f 293 -l 20 -D 0 -n -f 493 -l 20 -D 189 -n -f 184 -l 20 -D 0 -n -f 277 -l 20 -D 0 -n -f 466 -l 20 -D 47 -n -f 174 -l 20 -D 0 -n -f 261 -l 20 -D 0 -n -f 440 -l 20 -D 190 -n -f 164 -l 20 -D 0 -n -f 261 -l 20 -D 0 -n -f 391 -l 20 -D 95 -n -f 261 -l 20 -D 0 -n -f 391 -l 20 -D 0 -n -f 659 -l 20 -D 95 -n -f 329 -l 20 -D 0 -n -f 493 -l 20 -D 0 -n -f 783 -l 20 -D 95 -n -f 349 -l 20 -D 0 -n -f 523 -l 20 -D 0 -n -f 880 -l 20 -D 190 -n -f 293 -l 20 -D 0 -n -f 440 -l 20 -D 0 -n -f 698 -l 20 -D 47 -n -f 329 -l 20 -D 0 -n -f 493 -l 20 -D 0 -n -f 783 -l 20 -D 189 -n -f 261 -l 20 -D 0 -n -f 440 -l 20 -D 0 -n -f 659 -l 20 -D 189 -n -f 220 -l 20 -D 0 -n -f 329 -l 20 -D 0 -n -f 523 -l 20 -D 47 -n -f 246 -l 20 -D 0 -n -f 349 -l 20 -D 0 -n -f 587 -l 20 -D 47 -n -f 195 -l 20 -D 0 -n -f 293 -l 20 -D 0 -n -f 493 -l 20 -D 332 -n -f 195 -l 20 -D 0 -n -f 329 -l 20 -D 0 -n -f 523 -l 20 -D 332 -n -f 164 -l 20 -D 0 -n -f 261 -l 20 -D 0 -n -f 391 -l 20 -D 332 -n -f 130 -l 20 -D 0 -n -f 195 -l 20 -D 0 -n -f 329 -l 20 -D 332 -n -f 174 -l 20 -D 0 -n -f 261 -l 20 -D 0 -n -f 440 -l 20 -D 189 -n -f 195 -l 20 -D 0 -n -f 293 -l 20 -D 0 -n -f 493 -l 20 -D 189 -n -f 184 -l 20 -D 0 -n -f 277 -l 20 -D 0 -n -f 466 -l 20 -D 47 -n -f 174 -l 20 -D 0 -n -f 261 -l 